# Every IRI in the OPL namespace must have a seven digit ID.

PREFIX rdfs:  <http://www.w3.org/2000/01/rdf-schema#>

SELECT DISTINCT ?s ?label
WHERE {
  ?s ?p ?o .
  FILTER (STRSTARTS(str(?s), "http://purl.obolibrary.org/obo/OPL_"))
  FILTER (!regex(str(?s), "^http://purl.obolibrary.org/obo/OPL_\\d{7}$"))
  OPTIONAL { ?s rdfs:label ?label }
}
